{
  "name": "docker-ts",
  "version": "0.0.1",
  "type": "module",
  "license": "Apache-2.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      },
      "default": "./dist/index.mjs"
    },
    "./dist/*": {
      "types": "./dist/*.d.ts",
      "import": "./dist/*.mjs",
      "require": "./dist/*.cjs"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "generate": "openapi-generator-cli generate -i swagger.yaml -c openapi-config.yaml -o src",
    "build": "npm run generate && tsup",
    "test": "vitest run",
    "format": "prettier --write ."
  },
  "devDependencies": {
    "@openapitools/openapi-generator-cli": "^2.23.4",
    "prettier": "^3.6.2",
    "tsup": "^8.5.0",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  }
}
